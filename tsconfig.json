{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "moduleResolution": "node"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "services/utils/icons.js", "components/Layout/MainLayout.js", "pages/Main/404/404.js", "pages/Main/index.js", "pages/Panel/Admin/index.js", "pages/Panel/User/index.js", "pages/_app.js", "pages/index.js", "components/UI/Navbar.js", "components/UI/Footer.js"  ],
  "exclude": [
    "node_modules"
  ]
}
